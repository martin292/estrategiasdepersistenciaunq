<hibernate-mapping>
	<class name="Vuelo" table="vuelo">
		<id name="id" type="java.lang.Integer">
			<column name="id"/>
			<generator class="identity"/>
		</id>
		
		<property name="tramos" type="aerolinea.Tramo">
			<column name="idTramo" length="20" not-null="false" />
		</property>
		
	</class>
</hibernate-mapping>